<form theme="dark">
  <search id="systemPerformance">
    <query>
index=main host IN ($selectedHost$) AND source IN (cpu vmstat) AND sourcetype IN (cpu vmstat)
| fields *
    </query>
    <earliest>$ceis_time_range.earliest$</earliest>
    <latest>$ceis_time_range.latest$</latest>
  </search>
  <search id="webPerformance">
    <query>
index=prod_web host IN ($selectedHost$) source="/app/apache/httpd_rp/logs/access_log" sourcetype=access_combined 
| dedup _time host
| fields *
    </query>
    <earliest>$ceis_time_range.earliest$</earliest>
    <latest>$ceis_time_range.latest$</latest>
  </search>
  <label>CEIS - JAG reverse proxy - DEV</label>
  <fieldset submitButton="false">
    <input type="time" token="ceis_time_range">
      <label>Time range</label>
      <default>
        <earliest>-15m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="checkbox" token="selectedHost" searchWhenChanged="true">
      <label>Select a host</label>
      <choice value="stew.bcgov">Stew</choice>
      <choice value="seethe.bcgov">Seethe</choice>
      <choice value="simmer.bcgov">Simmer</choice>
      <initialValue>stew.bcgov,seethe.bcgov,simmer.bcgov</initialValue>
      <default>stew.bcgov,seethe.bcgov,simmer.bcgov</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <single>
        <search>
          <query>
| makeresults 
| eval title="System Performance Indicators" 
| eval colorCode=1
| stats values(colorCode) as colorCode by title
| eval range=if(isnotnull(title),"low","severe")
          </query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="field">title</option>
        <option name="height">68</option>
        <option name="rangeColors">["0x325b99","0x3c444d"]</option>
        <option name="useColors">0</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <title>System Performance</title>
      <chart>
        <title>CPU Load</title>
        <search base="systemPerformance">
          <query>
| timechart 
  avg(cpu_load_percent) as Load
  by host
          </query>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">collapsed</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">line</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">connect</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
